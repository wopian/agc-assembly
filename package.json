{
  "name": "agc-assembly",
  "displayName": "AGC Assembly",
  "version": "0.1.2",
  "description": "Syntax-highlighting for Apollo Guidance Computer (AGC) assembly source code",
  "badges": [
    {
      "url": "https://img.shields.io/github/commits-since/wopian/agc-assembly/0.1.2.svg?style=flat-square",
      "href": "https://david-dm.org/wopian/agc-assembly?type=dev",
      "description": "GitHub Commits"
    },
    {
      "url": "https://david-dm.org/wopian/agc-assembly/dev-status.svg?style=flat-square",
      "href": "https://david-dm.org/wopian/agc-assembly?type=dev",
      "description": "devDependency Status"
    }
  ],
  "engines": {
    "vscode": "^1.5.0",
    "node": "^7.2.1",
    "npm": "^4.0.3"
  },
  "publisher": "wopian",
  "license": "Apache-2.0",
  "categories": [
    "Languages"
  ],
  "keywords": [
    "Apollo Guidance Computer",
    "Assembly"
  ],
  "homepage": "https://github.com/wopian/agc-assembly/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/wopian/agc-assembly"
  },
  "bugs": {
    "url": "https://github.com/wopian/agc-assembly/issues"
  },
  "galleryBanner": {
    "color": "#0B3D91",
    "theme": "dark"
  },
  "icon": "images/icon.png",
  "activationEvents": [
    "onLanguage:agc"
  ],
  "main": "./lib/agc",
  "contributes": {
    "languages": [
      {
        "id": "agc",
        "aliases": [
          "AGC Assembly",
          "agc"
        ],
        "extensions": [
          ".agc"
        ],
        "configuration": "./languages/agc.json"
      },
      {
        "id": "ags",
        "aliases": [
          "AGS Assembly",
          "ags"
        ],
        "extensions": [
          ".ags",
          ".aea"
        ],
        "configuration": "./languages/ags.json"
      },
      {
        "id": "argus",
        "aliases": [
          "ARGUS H800 Assembly Language",
          "argus"
        ],
        "extensions": [
          ".argus",
          ".mitigus"
        ],
        "configuration": "./languages/argus.json"
      },
      {
        "id": "binsource",
        "aliases": [
          "AGC Binsource",
          "binsource"
        ],
        "extensions": [
          ".binsource"
        ],
        "configuration": "./languages/binsource.json"
      }
    ],
    "grammars": [
      {
        "language": "agc",
        "scopeName": "source.agc_assembly",
        "path": "./syntaxes/agc.tmLanguage"
      },
      {
        "language": "ags",
        "scopeName": "source.ags_assembly",
        "path": "./syntaxes/ags.tmLanguage"
      },
      {
        "language": "argus",
        "scopeName": "source.mitigus",
        "path": "./syntaxes/argus.tmLanguage"
      },
      {
        "language": "binsource",
        "scopeName": "text.binsource",
        "path": "./syntaxes/binsource.tmLanguage"
      }
    ]
  },
  "devDependencies": {
    "@types/mocha": "^2.2.35",
    "@types/node": "^6.0.54",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-preset-latest": "^6.16.0",
    "converter": "0.0.5",
    "eslint": "^3.12.2",
    "mocha": "^3.2.0",
    "typescript": "^2.1.4",
    "vscode": "^1.0.3"
  },
  "scripts": {
    "compile-build": "babel src/build -d lib",
    "compile-highlight": "babel src/highlight -d lib",
    "compile-test": "babel src/test -d test",
    "prestart": "npm run compile-build && npm run compile-highlight",
    "pretest": "npm run compile-test && node ./node_modules/vscode/bin/install",
    "start": "node ./lib/build.js",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test && mocha --compilers js:babel-core/register"
  }
}
