{
  "name": "agc-assembly",
  "displayName": "AGC Assembly",
  "version": "0.2.5",
  "description": "Syntax highlighting for Apollo Guidance Computer (AGC) assembly source code",
  "publisher": "wopian",
  "license": "MIT",
  "homepage": "https://github.com/wopian/agc-assembly/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/wopian/agc-assembly"
  },
  "bugs": {
    "url": "https://github.com/wopian/agc-assembly/issues"
  },
  "galleryBanner": {
    "color": "#0B3D91",
    "theme": "dark"
  },
  "icon": "img/icon.png",
  "categories": [
    "Languages"
  ],
  "keywords": [
    "Apollo Guidance Computer",
    "Assembly"
  ],
  "contributes": {
    "languages": [
      {
        "id": "agc",
        "aliases": [
          "AGC",
          "agc"
        ],
        "extensions": [
          ".agc"
        ],
        "configuration": "./out/syntax/agc.json"
      },
      {
        "id": "ags",
        "aliases": [
          "AGS",
          "ags"
        ],
        "extensions": [
          ".ags",
          ".aea"
        ],
        "configuration": "./out/syntax/ags.json"
      },
      {
        "id": "argus",
        "aliases": [
          "ARGUS Honeywell 800",
          "argus"
        ],
        "extensions": [
          ".argus",
          ".mitigus"
        ],
        "configuration": "./out/syntax/argus.json"
      },
      {
        "id": "binsource",
        "aliases": [
          "AGC Binsource",
          "binsource"
        ],
        "extensions": [
          ".binsource"
        ],
        "configuration": "./out/syntax/binsource.json"
      }
    ],
    "grammars": [
      {
        "language": "agc",
        "scopeName": "source.agc_assembly",
        "path": "./out/syntax/agc.tmlanguage"
      },
      {
        "language": "ags",
        "scopeName": "source.ags_assembly",
        "path": "./out/syntax/ags.tmlanguage"
      },
      {
        "language": "argus",
        "scopeName": "source.mitigus",
        "path": "./out/syntax/argus.tmlanguage"
      },
      {
        "language": "binsource",
        "scopeName": "text.binsource",
        "path": "./out/syntax/binsource.tmlanguage"
      }
    ],
    "configurationDefaults": {
      "[agc]": {
        "editor.detectIndentation": false,
        "editor.insertSpaces": false,
        "editor.tabSize": 8,
        "editor.trimAutoWhitespace": true,
        "editor.useTabStops": true,
        "editor.wordSeparators": "  ",
        "files.trimTrailingWhitespace": true
      },
      "[ags]": {
        "editor.detectIndentation": false,
        "editor.insertSpaces": false,
        "editor.tabSize": 8,
        "editor.trimAutoWhitespace": true,
        "editor.useTabStops": true,
        "editor.wordSeparators": "  ",
        "files.trimTrailingWhitespace": true
      },
      "[binsource]": {
        "editor.detectIndentation": false,
        "editor.insertSpaces": false,
        "editor.tabSize": 8,
        "editor.trimAutoWhitespace": true,
        "editor.useTabStops": true,
        "editor.wordSeparators": "  ",
        "files.trimTrailingWhitespace": true
      },
      "[argus]": {
        "editor.detectIndentation": false,
        "editor.insertSpaces": true,
        "editor.rulers": [1, 8, 19, 32, 46, 60, 75, 80, 120],
        "editor.tabSize": 8,
        "editor.trimAutoWhitespace": true,
        "editor.useTabStops": true,
        "files.trimTrailingWhitespace": true
      }
    }
  },
  "engines": {
    "vscode": ">=1.10.1",
    "node": ">=6.5.0",
    "npm": ">=3.10.3"
  },
  "devDependencies": {
    "@types/chai": "3.4.35",
    "@types/chai-as-promised": "0.0.29",
    "@types/mkdirp": "0.3.29",
    "@types/mocha": "2.2.39",
    "@types/node": "7.0.7",
    "@types/rimraf": "0.0.28",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "converter": "0.0.5",
    "mkdirp": "0.5.1",
    "mocha": "3.2.0",
    "q": "1.4.1",
    "rimraf": "2.6.1",
    "tslint": "4.5.1",
    "typescript": "2.2.1",
    "vscode": "1.0.5"
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "vscode:prepublish": "npm run b",
    "compile": "tsc -watch -p ./",
    "test": "node ./node_modules/vscode/bin/test --silent",
    "vscode:install": "node ./node_modules/vscode/bin/install",
    "b": "tsc -p ./ && node out/src/build.js"
  }
}
